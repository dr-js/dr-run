{
  "private": true,
  "name": "@dr-js/run",
  "version": "0.3.0",
  "description": "Server source for dr.run",
  "author": "dr-js",
  "license": "MIT",
  "repository": "github:dr-js/dr-run",
  "bin": {
    "dr-run": "bin/index.js",
    "DRR": "bin/index.js"
  },
  "scripts": {
    "// repo ========================": "",
    "version-bump": "dr-dev --version-bump-git-branch --git-commit --debug",
    "tag-push": "dr-dev --shell-alias quick-git-push-combo",
    "// script ======================": "",
    "script-pack": "node ./script/pack.js pack",
    "script-pack-test": "npm run script-pack -- verbose test",
    "script-publish": "npm run script-pack -- publish",
    "script-generate-spec": "node -r @babel/register ./script/generateSpec.js",
    "// build =======================": "",
    "build-library": "node ./script/webpack.js production profile",
    "build-library-dev": "node ./script/webpack.js development watch",
    "// test ========================": "",
    "test-dev": "dr-dev --EE BABEL_ENV=dev -E -- dr-dev --TR @babel/register -T source/module/WeblogMarkdown/generate.test.js",
    "test-source": "dr-dev --TR @babel/register -T source/ --TFS .test.js",
    "// =============================": "",
    "analyze-stat": "npx webpack-bundle-analyzer .temp-gitignore/profile-stat.json output-gitignore/library",
    "lint": "eslint ./source/ ./source-bin/ ./script/",
    "test": "npm run script-pack-test",
    "prepack": "echo \"Error: pack with script-*\" && exit 1"
  },
  "engines": {
    "node": ">=14.18",
    "npm": ">=6.14"
  },
  "devDependencies": {
    "@dr-js/core": "^0.5.0",
    "@dr-js/dev": "^0.5.0",
    "@dr-js/dev-web": "^0.5.0",
    "highlight.js": "^11.5.1",
    "marked": "^4.0.16"
  }
}
